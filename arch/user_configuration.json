{
    "additional-repositories": [
        "multilib"
    ],
    "audio": "pipewire",
    "bootloader": "grub-install",
    "config_version": "2.5.6",
    "debug": false,
    "gfx_driver": "All open-source (default)",
    "harddrives": [
        "/dev/vda"
    ],
    "hostname": "archbtw",
    "kernels": [
        "linux-hardened"
    ],
    "mirror-region": {
        "Greece": {
            "http://ftp.cc.uoc.gr/mirrors/linux/archlinux/$repo/os/$arch": true,
            "http://ftp.ntua.gr/pub/linux/archlinux/$repo/os/$arch": true,
            "http://ftp.otenet.gr/linux/archlinux/$repo/os/$arch": true,
            "https://repo.greeklug.gr/data/pub/linux/archlinux/$repo/os/$arch": true
        }
    },
    "nic": {
        "dhcp": true,
        "dns": null,
        "gateway": null,
        "iface": null,
        "ip": null,
        "type": "nm"
    },
    "no_pkg_lookups": false,
    "offline": false,
    "packages": [
        "git",
        "neovim"
    ],
    "profile": {
        "path": "/usr/lib/python3.11/site-packages/archinstall/profiles/xorg.py"
    },
    "script": "guided",
    "silent": false,
    "sys-encoding": "utf-8",
    "sys-language": "en_US",
    "version": "2.5.6"
}